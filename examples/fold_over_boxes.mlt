let rec box_n_values n boxed =
  if n = 0 
  then boxed 
  else box_n_values (n - 1) ((box 3 n*n) :: boxed)

run
  let boxed_vals = box_n_values 5 [] in
  let boxed_fun = 
    fold_left 
      (fun acc value -> 
         box 10 (fun () -> let unboxed_acc = unbox acc in 
                           let unboxed_value = unbox value in 
                           unboxed_acc () + unboxed_value)) 
      (box 10 (fun () -> 0))
      boxed_vals 
  in
  delay 10; 
  (unbox boxed_fun) ()