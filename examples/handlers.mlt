operation Get : unit ~> int # 71
operation Set : int ~> unit # 2

let h =
  handler
  | x -> 
      perform Set 42; 
      let y = perform Get () in
      x + y
  | Get () k -> 
      delay 71;
      let k' = unbox 71 k in
      k' 24
  | Set x k -> 
      perform Set x;
      let k' = unbox 2 k in
      k' ()

let h' =
  handler
  | x -> x * x
  | Get x k -> 
      delay 71;
      let k' = unbox 71 k in
      k' 42

run
  handle 
    perform Get ();
    return 3
  with h'