operation Get : unit ~> int # 71
operation Set : int ~> unit # 2

let h =
  handler
  | x -> 
      perform Set 42; 
      let y = perform Get () in
      x + y
  | Get () k -> 
      delay 71;
      let k' = unbox 71 k in
      k' 24
  | Set x k -> 
      perform Set x;
      let k' = unbox 2 k in
      k' ()

let foo () =
  handle 
    perform Set 4; 
    perform Get ();
    delay 2;
    1
  with h